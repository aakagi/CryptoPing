    "--NOT INTENDED TO BE RUN INDIVIDUALLY--": "",
    "clean": "",
    "build-web": "cd ./cryptoping-web && npm run build && cd ..",
    "hyper-clean-web": "hyper compose down -p ozmeum-web && hyper rmi ozmeum/ozmeum-web",
    "build-services": ""
  },
